{
  "name": "rest-api-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "description": "Xefi rest-api-react sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdelLyon/rest-api-react.git"
  },
  "author": "DailyApps",
  "license": "MIT",
  "exports": {
    "./rest-api/types": {
      "import": "./dist/rest-api/types/index.es.js",
      "types": "./dist/rest-api/types/index.d.ts"
    },
    "./rest-api/services": {
      "import": "./dist/rest-api/services/index.es.js",
      "types": "./dist/rest-api/services/index.d.ts"
    },
    "./rest-api/models": {
      "import": "./dist/rest-api/models/index.es.js",
      "types": "./dist/rest-api/models/index.d.ts"
    },
    "./rest-api/hooks": {
      "import": "./dist/rest-api/hooks/index.es.js",
      "types": "./dist/rest-api/hooks/index.d.ts"
    },
    "./rest-api/factories": {
      "import": "./dist/rest-api/factories/index.es.js",
      "types": "./dist/rest-api/factories/index.d.ts"
    },
    "./rest-api/di": {
      "import": "./dist/rest-api/di/index.es.js",
      "types": "./dist/rest-api/di/index.d.ts"
    },
    "./react-query": {
      "import": "./dist/react-query/index.es.js",
      "types": "./dist/react-query/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "rest-api/*": [
        "./dist/rest-api/*/index.d.ts"
      ],
      "react-query": [
        "./dist/react-query/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf node_modules dist *.log && pnpm install",
    "build": "pnpm lint --fix && tsc -b && vite build",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "lint": "eslint .",
    "preview": "vite preview",
    "reset": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.64.1",
    "@tanstack/react-query-devtools": "^5.64.1",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "cookies-next": "^5.0.2",
    "reflect-metadata": "^0.2.2"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tanstack/config": "^0.15.0",
    "@types/node": "^22.10.3",
    "@types/react": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}
