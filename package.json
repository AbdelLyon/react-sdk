{
  "name": "rest-api-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "description": "Xefi rest-api-client sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdelLyon/rest-api-react.git"
  },
  "author": "DailyApps",
  "license": "MIT",
  "exports": {
    "./types": {
      "import": "./dist/types/index.es.js",
      "types": "./dist/types/index.d.ts"
    },
    "./services": {
      "import": "./dist/services/index.es.js",
      "types": "./dist/services/index.d.ts"
    }
