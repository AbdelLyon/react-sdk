  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tanstack/config": "^0.15.0",
    "@types/node": "^22.10.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.6",
